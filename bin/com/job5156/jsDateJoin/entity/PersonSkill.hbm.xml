<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.job5156.jsDateJoin.entity.PersonSkill" dynamic-insert="true" dynamic-update="true"  table="user_skill">
    <id name="id" type="java.lang.Integer" unsaved-value="null" >
		<column name="skillid" not-null="true"/>
		<generator class="native"/>
	</id>
	<property name="userid" />
	<property name="techtitle" />
	<property name="computerlevel" />
	<property name="computerskill" />
	<property name="certificate1" />
	<property name="language1" />
	<property name="language2" />
	<property name="certificate2" />
	<property name="certificate" />
	<!--property name="chineselevel" />
	<property name="cantoneselevel" />
	<property name="language3" /-->
	<property name="otherskills" />
	<property name="cre_date" />
	<property name="rev_date" />
	
</class>
</hibernate-mapping>
