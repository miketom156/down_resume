<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.job5156.jsDateJoin.entity.PersonIntent" dynamic-insert="true" dynamic-update="true"  table="user_intent">
    <id name="id" type="java.lang.Integer" unsaved-value="null" >
		<column name="intentid" not-null="true"/>
		<generator class="native"/>
	</id>
	<property name="userid" />
	
	<property name="workyear" />
	<property name="workyear1" />
	<property name="workcalling" />
	<property name="workcalling1" />
		
	<property name="jobcode1" />
	<property name="jobcode2" />
	<property name="jobcode3" />
	
	<property name="state" />
     
	<property name="otherjob1" />
	<property name="otherjob2" />
	<property name="otherjob3" />
	
	<property name="joblocation1" />
	<property name="joblocation2" />
	<property name="joblocation3" />
	<property name="salary" />
	<property name="neededhouse" />
	<property name="checkindate" />
	<property name="checkindate1" />
	<property name="otherrequirement" />
	<property name="rev_date" />
	<property name="cre_date" />
	
</class>
</hibernate-mapping>
