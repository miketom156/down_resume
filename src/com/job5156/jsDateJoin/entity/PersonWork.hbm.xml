<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.job5156.jsDateJoin.entity.PersonWork" dynamic-insert="true" dynamic-update="true"  table="user_work">
    <id name="id" type="java.lang.Integer" unsaved-value="null" >
		<column name="workid" not-null="true"/>
		<generator class="native"/>
	</id>
	<property name="userid" />
	<property name="begindateyear" />
	<property name="begindatemonth" />
	<property name="enddateyear" />
	<property name="enddatemonth" />
	<property name="comname" />
	<property name="comtype" />
	<property name="comcalling" />
	<property name="maincatalog" />
	<property name="jobfunctionid" />
	<property name="salary" />
	<property name="otherposition" />
	<property name="description" />
	<property name="leftreason" />
	<property name="cre_date" />
	<property name="rev_date" />
	<property name="flag" />
	
</class>
</hibernate-mapping>
