	public String getJob5156Code(String[][] checkList, String value,boolean isContaint)
	{
		String code = "";
		if(isContaint == true)
		{
			String initValue = value;
			for (String[] array : checkList)
			{
				if (initValue.equals(array[0]))
				{
					code = StringUtil.getNotNullStr(array[2]);
					break;
				}
			}
		}
		return code;  
	}
	
